{
  "name": "zentlix/main-bundle",
  "type": "symfony-bundle",
  "description": "Zentlix Main bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.4.0",
    "symfony/framework-bundle": "4.4.*",
    "sensio/framework-extra-bundle": "^5.5",
    "symfony/orm-pack": "^1.0",
    "symfony/translation": "4.4.*",
    "symfony/form": "4.4.*",
    "symfony/validator": "4.4.*",
    "symfony/twig-pack": "^1.0",
    "symfony/expression-language": "4.4.*",
    "symfony/messenger": "4.4.*",
    "symfony/http-client": "4.4.*",
    "omines/datatables-bundle": "^0.4.2",
    "limenius/liform-bundle": "^0.15.0",
    "tightenco/collect": "^7.9",
    "gedmo/doctrine-extensions": "^2.4",
    "ramsey/uuid": "^4.0",
    "ramsey/uuid-doctrine": "^1.6",
    "knplabs/knp-paginator-bundle": "^5.3",
    "composer/composer": "^2.0",
    "zentlix/user-bundle": "^1.1"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Zentlix\\MainBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
