{% extends '@MainBundle/admin/layouts/base.html.twig' %}

{% block title %}{{ 'zentlix_main.bundle.update.process' | trans }}{% endblock %}

{% block breadcrumbs %}
    <li class="breadcrumb-item"><a href="{{ path('admin.index') }}">{{ 'zentlix_main.control_panel' | trans }}</a></li>
    <li class="breadcrumb-item active">{{ 'zentlix_main.bundle.update.process' | trans }}</li>
{% endblock %}

{% block body %}
    <div class="row">
        <div class="card w-100">
            <div class="card-header">
                {{ 'zentlix_main.bundle.update.process' | trans }}
            </div>
            <div class="card-body">
                <div class="alert alert-info">{{ 'zentlix_main.bundle.without_settings' | trans }}</div>
            </div>
            <div class="card-footer">
                <a href="{{ path('admin.bundle.list') }}" class="btn btn-sm btn-secondary" type="button">{{ 'zentlix_main.cancel' | trans }}</a>
            </div>
        </div>
    </div>
{% endblock %}